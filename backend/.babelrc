{
  "presets": [
    "@babel/preset-typescript",
    "@babel/preset-env"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "extensions": [
          ".ts",
          ".tsx"
        ],
        "alias": {
          "@src": "./src"
        }
      }
    ]
  ]
}